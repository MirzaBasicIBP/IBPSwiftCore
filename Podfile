# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'IBPSwiftCore' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for IBPSwiftCore
  pod 'Alamofire'
  pod 'ObjectMapper'

  target 'IBPSwiftCoreTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire'
    pod 'ObjectMapper'
  end

end

target 'IBPSwiftCorePackageDescription' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for IBPSwiftCorePackageDescription

end
